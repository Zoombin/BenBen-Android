        <item name="android:gravity">center</item>
        <item name="android:textSize">14dp</item>
        <item name="android:clickable">false</item>
    </style>

    <style name="AppTheme_s" parent="android:Theme.Light">
        <item name="android:windowBackground">@drawable/transport</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="login_theme" parent="android:Theme.Light">
